<odoo>
    <data>
        <record id="order_kerja_produksi_paperformat" model="report.paperformat">
            <field name="name">Order Kerja Produksi(OKP)</field>
            <field name="default" eval="True"/>
            <field name="format">custom</field>
            <field name="page_height">280</field>
            <field name="page_width">220</field>
            <field name="orientation">Landscape</field>
            <field name="margin_top">0</field>
            <field name="margin_bottom">10</field>
            <field name="margin_left">5</field>
            <field name="margin_right">5</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">0</field>
            <field name="dpi">80</field>
        </record>

        <record id="hasil_produksi_paperformat" model="report.paperformat">
            <field name="name">Report Hasil Produksi</field>
            <field name="default" eval="True"/>
            <field name="format">custom</field>
            <field name="page_height">280</field>
            <field name="page_width">220</field>
            <field name="orientation">Landscape</field>
            <field name="margin_top">0</field>
            <field name="margin_bottom">10</field>
            <field name="margin_left">5</field>
            <field name="margin_right">5</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">0</field>
            <field name="dpi">80</field>
        </record>
    </data>
</odoo>