<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <record id="view_users_form_inehrit_bmo_inventory" model="ir.ui.view">
            <field name="name">res.users.form.inherit.bmo.inventory</field>
            <field name="model">res.users</field>
            <field name="inherit_id" ref="base.view_users_form"/>
            <field name="arch" type="xml">
                <xpath expr="//page[@name='access_rights']/group[1]" position="before">
                    <group name="warehouse" string="Warehouse">
                        <field name="picking_type_ids" widget="many2many_tags"/>
                        <field name="location_id" widget="many2many_tags"/>
                    </group>
                </xpath>
            </field>
        </record>
    </data>
</odoo>