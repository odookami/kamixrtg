                </xpath>
                <xpath expr="//field[@name='product_uom_id']" position="after">
                    <field name="verifikator_admin" required="1" attrs="{'column_invisible': [('parent.picking_type_code','!=','outgoing')]}"/>
                    <field name="qty_used_okp" attrs="{'column_invisible': [('parent.tipe','!=','Mixing'),('parent.custom_requisition_id','!=',False)]}" optional="hide"/>
                </xpath>
            </field>
