        <record id="view_stock_move_line_operations_inherit_bmo_material_request" model="ir.ui.view">
            <field name="name">stock.move.line.operations.form.inherit.bmo_material_request</field>
            <field name="model">stock.move.line</field>
            <field name="inherit_id" ref="stock.view_stock_move_line_detailed_operation_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='product_uom_id']" position="after">
                    <field name="verifikator_check" invisible='1'/>
                    <field name="verifikator" required="1" attrs="{'invisible' : [('verifikator_check','=', False)],
