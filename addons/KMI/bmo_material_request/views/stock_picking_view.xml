        <record id="vpicktree_inherit_bmo_mr" model="ir.ui.view">
            <field name="name">stock.picking.tree.form.inherit.bmo.mr</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.vpicktree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='origin']" position="after">
                    <field name="tipe" readonly="1"/>
                    <field name="batch_production_id"/>
                    <field name="product_batch_id" />
                    <field name="batch_proses" />
                    <field name="uom_id" />
                </xpath>
            </field>
        </record>
