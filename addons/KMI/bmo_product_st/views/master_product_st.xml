<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data noupdate="0">

        <record id="model_name_view_tree" model="ir.ui.view">
            <field name="name">master.product.st.view.tree</field>
            <field name="model">master.product.st</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="product_id"/>
                    <field name="name"/>
                    <field name="number"/>
                    <field name="unit"/>
                    <field name="parameter"/>
                </tree>
            </field>
        </record>

        <record id="master_product_st_action" model="ir.actions.act_window">
            <field name="name">Master Pasteurisasi</field>
            <field name="res_model">master.product.st</field>
            <field name="view_mode">tree</field>
        </record>

        <!-- <menuitem id="menu_config_record_product_st" parent="bmo_batch_record.menu_setting_batch_record" action="master_product_st_action" sequence="10" /> -->

